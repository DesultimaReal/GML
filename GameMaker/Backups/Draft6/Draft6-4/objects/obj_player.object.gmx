///Level up code
with(object_player_stats){
    expr += 1;
    if(expr &gt;= maxexpr) {
        level += 1;
        expr = expr-maxexpr;
        //Leveling Curve
        if(maxexpr &gt; 20){
            maxexpr = round(maxexpr * 1.2); 
        }
        else if(maxexpr &gt; 10){
            maxexpr = round(maxexpr * 1.1); 
        }
        else{
            maxexpr = maxexpr + 1
        }
        //////////////////////////
